MATLAB_ROOT = C:\PROGRA~2\MATLAB\R2015b
MAKEFILE = calculateTransformations_mex.mk

include calculateTransformations_mex.mki


SRC_FILES =  \
	calculateTransformations_data.c \
	calculateTransformations_initialize.c \
	calculateTransformations_terminate.c \
	calculateTransformations.c \
	rot2euler.c \
	error.c \
	_coder_calculateTransformations_info.c \
	_coder_calculateTransformations_api.c \
	_coder_calculateTransformations_mex.c

MEX_FILE_NAME_WO_EXT = calculateTransformations_mex
MEX_FILE_NAME = $(MEX_FILE_NAME_WO_EXT).mexw32
TARGET = $(MEX_FILE_NAME)

SYS_LIBS = 


